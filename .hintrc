{
  "extends": [
    "development"
  ],
  "hints": {
    "no-inline-styles": "off",
    "button-type": "off",
    "axe/keyboard": [
      "default",
      {
        "nested-interactive": "off"
      }
    ],
    "compat-api/html": [
      "default",
      {
        "ignore": [
          "img[loading]"
        ]
      }
    ],
    "axe/forms": "off",
    "axe/parsing": "off"
  },
  "browserslist": [
    "defaults",
    "not ie 11",
    "not and_ff <= 106",
    "not firefox <= 107",
    "not ios_saf <= 16.1",
    "not safari <= 16.1"
  ]
}